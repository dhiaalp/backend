{
  "packageManager": "pnpm@9.15.0",
  "name": "artx",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && node src/scripts/postBuild.js",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "cd .medusa/server && medusa start --verbose",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.12.3",
    "@medusajs/admin-shared": "^2.12.3",
    "@medusajs/auth-emailpass": "^2.12.3",
    "@medusajs/cli": "2.12.3",
    "@medusajs/dashboard": "2.12.3",
    "@medusajs/draft-order": "^2.12.3",
    "@medusajs/file": "^2.12.3",
    "@medusajs/file-local": "^2.12.3",
    "@medusajs/framework": "2.12.3",
    "@medusajs/icons": "^2.12.3",
    "@medusajs/medusa": "2.12.3",
    "@medusajs/payment-stripe": "2.12.3",
    "@medusajs/ui": "^4.0.31",
    "minio": "^8.0.6",
    "ulid": "^2.4.0"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.3",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
